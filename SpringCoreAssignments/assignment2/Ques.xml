<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
     
<bean class="com.assignment2.QuestionList" name="QuestionList">

   <property name="questionId">
     <value>01</value>
   </property>
  
   <property name="question" >
     <value>What is your name</value>
   </property>
  
   <property name="answers">
    <list>
       <value>Muskaan</value>
       <value>Ruchi</value>
    </list>
   </property>
</bean>
            
<bean class="com.assignment2.QuestionMap" name="QuestionMap">
 
 <property name="questionId">
   <value>01</value>
 </property>
  
 <property name="question" >
   <value>What is your name</value>
 </property>
  
 <property name="answers">
   <map>
      <entry key="01" value="Muskaan"></entry>
      <entry key="02" value="Ruchi"></entry>
   </map>
 
 </property>
</bean>
<bean class="com.assignment2.QuestionSet" name="QuestionSet">

   <property name="questionId">
     <value>01</value>
   </property>
  
   <property name="question" >
     <value>What is your name</value>
   </property>
  
   <property name="answers">
    <set>
       <value>Muskaan</value>
       <value>Ruchi</value>
    </set>
   </property>
</bean>
</beans>